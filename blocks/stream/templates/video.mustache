<div>
    {{# addcapability}}
    
        <a title="{{#str}}addvideo, block_stream{{/str}}" class="btn btn-primary pull-right upload_btn" href="javascript:void(0);" onclick="(function(e){ require('block_stream/upload').init({selector:'createcourseenrolmodal', context:1, id:0 })})(event)">
          <i class="fa fa-arrow-up px-2" aria-hidden="true"></i>
        Upload Video</a>
        
    {{/ addcapability}}
    <ul class="nav nav-tabs md-pills pills-secondary">
        {{# content }}
        <li class="nav-item">
            <a class="nav-link active" data-toggle="tab" href="#panel11" role="tab">{{# str}}uploadedvideos, block_stream {{/ str}}</a>
        </li>
        {{/ content }}
        <li class="nav-item">
            <a class="nav-link {{^ content }} active {{/ content }}" data-toggle="tab" href="#panel12" role="tab">{{# str}}totalvideos, block_stream {{/ str}}</a>
        </li>
    </ul> 
    <div class="tab-content pt-0">
        {{# content }}
        <div class="tab-pane fade in show active" id="panel11" role="tabpanel">
          <br>
          {{{content}}}
        </div>
        {{/ content }}
        <div class="tab-pane fade {{^ content }} in show active {{/ content }}" id="panel12" role="tabpanel">
            <br>
              {{{stream}}}
        </div>
    </div>
</div>